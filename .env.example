# Environment variables template
# Copy this file to .env and fill in your actual values

# Database
# For development, SQLite is used (automatically handled by Prisma)
# The database file will be created at prisma/dev.db
DATABASE_URL="file:./dev.db"

# NextAuth.js
# Generate a random secret: openssl rand -base64 32
NEXTAUTH_SECRET="your-secret-key-here"
NEXTAUTH_URL="http://localhost:3000"

# Optional: Email configuration (for future use)
# SMTP_HOST=
# SMTP_PORT=
# SMTP_USER=
# SMTP_PASSWORD=
# SMTP_FROM=

# Optional: Cloud storage (for future image uploads)
# AWS_ACCESS_KEY_ID=
# AWS_SECRET_ACCESS_KEY=
# AWS_REGION=
# AWS_S3_BUCKET=

# 경매 데이터 크롤링 설정
# 마이옥션 (메인 데이터 소스 - 임차인, 등기부, 배당표, 국토부실거래가 무료)
MYAUCTION_BASE_URL="https://www.my-auction.co.kr"
MYAUCTION_USERNAME="your-username"
MYAUCTION_PASSWORD="your-password"

# 두인경매 (사진 크롤링용 - 물건 사진이 풍부함)
DOOIN_BASE_URL="https://www.dooinauction.com"
DOOIN_USERNAME="your-username"
DOOIN_PASSWORD="your-password"
