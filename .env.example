# Environment variables template
# Copy this file to .env and fill in your actual values

# Database
# For development, SQLite is used (automatically handled by Prisma)
# The database file will be created at prisma/dev.db
DATABASE_URL="file:./dev.db"

# NextAuth.js
# Generate a random secret: openssl rand -base64 32
NEXTAUTH_SECRET="your-secret-key-here"
NEXTAUTH_URL="http://localhost:3000"

# Optional: Email configuration (for future use)
# SMTP_HOST=
# SMTP_PORT=
# SMTP_USER=
# SMTP_PASSWORD=
# SMTP_FROM=

# Optional: Cloud storage (for future image uploads)
# AWS_ACCESS_KEY_ID=
# AWS_SECRET_ACCESS_KEY=
# AWS_REGION=
# AWS_S3_BUCKET=

# 공공데이터포털 API (청약홈 + 실거래가)
# https://www.data.go.kr 에서 발급
DATA_GO_KR_API_KEY="your-api-key-here"

# 카카오 지도 API
# https://developers.kakao.com 에서 발급
# 애플리케이션 등록 후:
# - JavaScript 키 → 프론트엔드 지도 표시용
# - REST API 키 → 서버사이드 지오코딩용 (주소→좌표 변환)
NEXT_PUBLIC_KAKAO_MAP_API_KEY="your-kakao-javascript-key"
KAKAO_REST_API_KEY="your-kakao-rest-api-key"

# 경매 데이터 크롤링 설정
# 마이옥션 (메인 데이터 소스 - 임차인, 등기부, 배당표, 국토부실거래가 무료)
MYAUCTION_BASE_URL="https://www.my-auction.co.kr"
MYAUCTION_USERNAME="your-username"
MYAUCTION_PASSWORD="your-password"

# 두인경매 (사진 크롤링용 - 물건 사진이 풍부함)
DOOIN_BASE_URL="https://www.dooinauction.com"
DOOIN_USERNAME="your-username"
DOOIN_PASSWORD="your-password"
